pipeline{
    agent any
    stages{
        stage("Git Checkout"){
            steps{ 
                git credentialsId: 'git', url: 'https://github.com/kumarsandeep590/mytest'
                
            }
            
        }
        stage("Maven Build'){
        steps{ 
        sh "mvn clean package"
        }
        }
        
        
    }
    
}
